# abort script on any command that exit with a non zero value
set -e

archive=`echo wso2is_analytics/wso2is-analytics-5.4.1*.zip`

if [[ -f $archive ]] ; then
  echo "wso2is-analytics-5.4.1 archive found"
else
  echo "wso2is-analytics-5.4.1 archive not found"
  exit 1
fi

cp -r $archive ${BOSH_INSTALL_TARGET}
